.user_profile .checkout_form_data .form-group {width: 46% !important;margin-right: 7% !important;}
.user_profile .checkout_form_data .form-group:nth-child(2n+0) {margin-right: 0 !important;}
form.checkout_form_data .form-group:nth-child(3n+0) {margin-right: 4% } 
form.checkout_form_data .form-group:nth-child(2n+0) {margin-right: 0 }
.Checkout_Form .checkout_form_data .form-group {width: 48%;}
}
@media screen and (max-width: 680px) {
.home_slider h5 {font-size: 23px;}
.home_slider p {font-size: 15px;line-height: 17px;}
.home_slider p button {background-color: #ff7f33;color: #fff;padding: 7px 16px;display: table;margin-top: 15px;border-color: transparent;font-size: 15px;}
.carousel-root .carousel .control-dots li.dot::before {font-size: 22px;}
.carousel-root .carousel .control-dots li.dot:not(:first-child)::after {left: -6px;font-size: 21px;}
}
@media screen and (max-width: 576px) {
.with_hund_pt {width: 100%;}
.home_slider h5 {font-size: 16px;}
.home_slider {margin-top: -103px;}
.carousel-root .carousel .control-dots:after {display: none;}
.search_div .tab_btn {padding: 0 11px;}
button.mobile_register_loginout_login {padding: 2px 12px;}
.cart_btn_dt img {width: 23px;}
.cart_btn_dt p.queant {padding: 2px 4px;font-size: 11px;}
.cart {margin-right: 15px;}
button.mobile_register_loginout_login {margin-left: 15px;}
.register_or_login .nav-link {padding: 4px 20px;}
.user_profile .checkout_form_data .form-group {width: 100% !important;margin-right: 0 !important;}
.form-common-main form .form_contact {width: 100% !important;margin: 0;}
.form-common-main form .btn {margin-top: 20px;}
.stripe-inner form.stripe_from {margin: 0 auto 30px;padding: 1px 10px;width: 300px;}
form.stripe_from .Card_Number input {width: 84%;}
span.input-group-addon {padding: 7.5px 12px;margin-top: -4px;display: inline-block;}
.submit_cart {margin: 10px 0;}
.month_Year {width: 49%;float: left;}
.stripe-inner .cvc_cvv label.form-label {width: 100%;}
.stripe-inner .cvc_cvv input {width: 100%;}
.register_loginout_login button.dropdown-toggle {padding: 0.175rem 0.55rem;}
.cvc_cvv .srv-validation-message {width: 100%;float: left;}
.cvc_cvv, .cvc_cvv input {width: 46%;float: right;margin-left: 13px;}
}
@media screen and (max-width:420px) {
.home_slider {margin-top: -93px;}
.home_slider h5 {font-size: 14px;}
.category-listing {padding: 0 4px;position: relative;width: 100%;}
.nab_left a.navbar-brand {width: 125px !important;}
.nab_left .navbar-brand img {width: 105px;}
.navbar-collapse .nab_right {width: auto;}
.togial_menu_div {padding-left: 1rem;}
}
